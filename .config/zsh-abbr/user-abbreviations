abbr "git s"="git status"
abbr "git cof"="git branch | grep -v \* | fzf --ansi --height 20% | xargs git switch"
abbr "git swf"="git branch | grep -v \* | fzf --ansi --height 20% | xargs git switch"
abbr "git brcl"="git fetch --prune; git branch | grep -v -e '*' -v -e 'master' -v -e 'main' | xargs -L1 git branch -d"
